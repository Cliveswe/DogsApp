@using DogsApp.Models
@model IEnumerable<Dog>
<!DOCTYPE html>
    <html>
    <head>
        <title>Dogs</title>
        <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.6/dist/css/bootstrap.min.css" />
    </head>
    <body>
        <ul>
            @foreach (var dog in Model)
            {
                <li>@dog.Name</li>
            }
        </ul>

        <a href="/create">Add</a>
    </body>
    </html>